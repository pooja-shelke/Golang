# Golang
//Write a program which accept one number from user and print that number of
//even numbers on screen.

package main

import "fmt"

func DisplyEvn(iValue int){

	var i int=0
	var iAns int=0

	for i=1;i<=iValue;i++{

		iAns=i%2
		if(iAns==0){

			fmt.Println(i)
		}
	}
}

func main(){
	var iValue int=0

	fmt.Println("Enter number")
	fmt.Scanln(&iValue)

	DisplyEvn(iValue)
}
