# Golang//Accept one number and check whether is is divisible by 5 or not.

package main

import "fmt"

func Division(iValue int)bool{

	var iAns int=0;

    iAns=(iValue%5)

    if(iAns==0){

    	return true
    }else{

    	return false
    }
}

func main(){
       
       var iValue int=0
       var bResult bool

       fmt.Println("Enter Number")
       fmt.Scanln(&iValue)

       bResult=Division(iValue)

       if(bResult==true){

       	fmt.Println("Number is Divisible by 5")
       }else{

       	fmt.Println("Number is not Divisible by 5")
       }
}
