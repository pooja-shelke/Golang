# Golang

//1.Program to divide two numbers

package main

import "fmt"

func Divide(iValue1 int,iValue2 int)int{
	 var iResult int=0

	 iResult=iValue1/iValue2                              //To perform Addition

	 return iResult                                       //return Addition
}


func main(){                                            //Entry point function

	var iValue1 int=0                                      //LOcal variables
	var iValue2 int=0
	var iResult int=0

	fmt.Println("Enter First number")
	fmt.Scanln(&iValue1)

	fmt.Println("Enter Second NUmber")
	fmt.Scanln(&iValue2)

	iResult=Divide(iValue1,iValue2)                      //Call to function

	fmt.Println("Division is :",iResult)                  //Return output

}
